{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"color": {
			"palette": [
				{
					"slug": "primary",
					"color": "#dc1b36",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#000000",
					"name": "Secondary"
				},
				{
					"slug": "dark-blue",
					"color": "#1a2b6a",
					"name": "Dark Blue"
				},
				{
					"slug": "light-gray",
					"color": "#f8f8f8",
					"name": "Light Gray"
				},
				{
					"slug": "medium-gray",
					"color": "#e5e5e5",
					"name": "Medium Gray"
				},
				{
					"slug": "dark-gray",
					"color": "#333333",
					"name": "Dark Gray"
				},
				{
					"slug": "white",
					"color": "#ffffff",
					"name": "White"
				},
				{
					"slug": "black",
					"color": "#000000",
					"name": "Black"
				}
			],
			"gradients": [
				{
					"slug": "primary-to-secondary",
					"gradient": "linear-gradient(135deg, #dc1b36 0%, #0a0a2e 100%)",
					"name": "Primary to Secondary"
				},
				{
					"slug": "blue-gradient",
					"gradient": "linear-gradient(135deg, #1a2b6a 0%, #0a0a2e 100%)",
					"name": "Blue Gradient"
				}
			],
			"custom": true,
			"customGradient": true,
			"defaultPalette": false,
			"defaultGradients": false
		},
		"typography": {
			"fontFamilies": [
				{
					"fontFamily": "Jost, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"slug": "jost",
					"name": "Jost"
				},
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"slug": "system-font",
					"name": "System Font"
				}
			],
			"fontSizes": [
				{
					"slug": "small",
					"size": "14px",
					"name": "Small"
				},
				{
					"slug": "normal",
					"size": "16px",
					"name": "Normal"
				},
				{
					"slug": "medium",
					"size": "20px",
					"name": "Medium"
				},
				{
					"slug": "large",
					"size": "24px",
					"name": "Large"
				},
				{
					"slug": "x-large",
					"size": "32px",
					"name": "X-Large"
				},
				{
					"slug": "xx-large",
					"size": "42px",
					"name": "XX-Large"
				},
				{
					"slug": "huge",
					"size": "54px",
					"name": "Huge"
				}
			],
			"customFontSize": true
		},
		"spacing": {
			"units": [
				"px",
				"em",
				"rem",
				"%",
				"vh",
				"vw"
			],
			"customSpacingSize": true
		},
		"layout": {
			"contentSize": "1200px",
			"wideSize": "1400px"
		},
		"custom": {
			"spacing": {
				"small": "10px",
				"medium": "20px",
				"large": "40px",
				"x-large": "60px",
				"xx-large": "80px"
			},
			"typography": {
				"line-height": {
					"heading": 1.2,
					"body": 1.6
				}
			},
			"border": {
				"radius": {
					"small": "5px",
					"medium": "10px",
					"large": "15px",
					"round": "50%"
				}
			}
		}
	},
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--white)",
			"text": "var(--wp--preset--color--dark-gray)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--jost)",
			"fontSize": "var(--wp--preset--font-size--normal)",
			"lineHeight": "var(--wp--custom--typography--line-height--body)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--jost)",
					"fontSize": "var(--wp--preset--font-size--huge)",
					"lineHeight": "var(--wp--custom--typography--line-height--heading)",
					"fontWeight": "700"
				},
				"color": {
					"text": "var(--wp--preset--color--secondary)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--jost)",
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"lineHeight": "var(--wp--custom--typography--line-height--heading)",
					"fontWeight": "700"
				},
				"color": {
					"text": "var(--wp--preset--color--secondary)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--jost)",
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"lineHeight": "var(--wp--custom--typography--line-height--heading)",
					"fontWeight": "600"
				},
				"color": {
					"text": "var(--wp--preset--color--secondary)"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--jost)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"lineHeight": "var(--wp--custom--typography--line-height--heading)",
					"fontWeight": "600"
				},
				"color": {
					"text": "var(--wp--preset--color--secondary)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"button": {
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--white)"
				},
				"border": {
					"radius": "var(--wp--custom--border--radius--small)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--jost)",
					"fontWeight": "600"
				}
			}
		},
		"blocks": {
			"core/button": {
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--white)"
				},
				"border": {
					"radius": "var(--wp--custom--border--radius--small)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--jost)",
					"fontWeight": "600"
				}
			},
			"core/navigation": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--jost)",
					"fontSize": "var(--wp--preset--font-size--normal)",
					"fontWeight": "500"
				},
				"color": {
					"text": "var(--wp--preset--color--secondary)"
				}
			},
			"core/heading": {
				"spacing": {
					"margin": {
						"top": "var(--wp--custom--spacing--large)",
						"bottom": "var(--wp--custom--spacing--medium)"
					}
				}
			}
		}
	}
}
