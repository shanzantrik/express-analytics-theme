{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "express-analytics/adminlte-multi-tabs",
    "version": "1.0.0",
    "title": "AdminLTE Multi Tabs",
    "category": "express-analytics",
    "icon": "grid-view",
    "description": "A custom block with vertical tabs styled like AdminLTE sidebar",
    "keywords": ["tabs", "adminlte", "vertical tabs"],
    "supports": {
        "html": false,
        "anchor": true,
        "align": ["wide", "full"]
    },
    "attributes": {
        "tabs": {
            "type": "array",
            "default": [
                {
                    "title": "Tab 1",
                    "icon": "fas fa-home",
                    "content": ""
                },
                {
                    "title": "Tab 2",
                    "icon": "fas fa-cog",
                    "content": ""
                }
            ]
        },
        "backgroundColor": {
            "type": "string",
            "default": "#343a40"
        },
        "textColor": {
            "type": "string",
            "default": "#ffffff"
        },
        "activeTabColor": {
            "type": "string",
            "default": "#007bff"
        }
    },
    "textdomain": "express-analytics",
    "editorScript": "file:../../../../build/adminlte-multi-tabs/block.js",
    "editorStyle": "file:./style.css",
    "style": "file:./style.css",
    "viewScript": "file:./frontend.js"
}
