{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "express-analytics/services",
    "version": "1.0.0",
    "title": "Services Grid",
    "category": "express-analytics",
    "icon": "grid-view",
    "description": "Display services in a grid layout with icons and descriptions.",
    "supports": {
        "html": false,
        "align": ["wide", "full"]
    },
    "attributes": {
        "heading": {
            "type": "string",
            "default": "Our Services"
        },
        "description": {
            "type": "string",
            "default": "Explore our comprehensive analytics services"
        },
        "columns": {
            "type": "number",
            "default": 3
        },
        "services": {
            "type": "array",
            "default": []
        },
        "backgroundColor": {
            "type": "string"
        },
        "textColor": {
            "type": "string"
        },
        "gradient": {
            "type": "string"
        }
    },
    "textdomain": "express-analytics",
    "editorScript": "file:../../../../build/services/block.js",
    "editorStyle": "file:./index.css",
    "style": "file:./style-index.css"
}
