{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "express-analytics/contact",
    "version": "1.0.0",
    "title": "Contact Form",
    "category": "express-analytics",
    "icon": "email",
    "description": "Display a contact form with custom fields and styling.",
    "supports": {
        "html": false,
        "align": ["wide", "full"]
    },
    "attributes": {
        "heading": {
            "type": "string",
            "default": "Get in Touch"
        },
        "description": {
            "type": "string",
            "default": "Have questions? We'd love to hear from you."
        },
        "formId": {
            "type": "string",
            "default": ""
        },
        "backgroundColor": {
            "type": "string"
        },
        "textColor": {
            "type": "string"
        },
        "gradient": {
            "type": "string"
        }
    },
    "textdomain": "express-analytics",
    "editorScript": "file:../../../../build/contact/block.js",
    "editorStyle": "file:./index.css",
    "style": "file:./style-index.css"
}
